{
    "name": "blox-it",
    "main": "src/bot.ts",
    "scripts": {
        "start": "yarn node --optimize_for_size --gc_interval=100 --max_old_space_size=920 dist/bot.js",
        "deploy": "yarn node dist/deploy.js",
        "build": "yarn swc ./src -d dist --delete-dir-on-start",
        "dev": "NODE_ENV=development && yarn build && yarn deploy &&  yarn start",
        "type-check": "yarn tsc",
        "lint": "eslint \"src/**/*.ts\"",
        "prettier": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
        "axios": "^1.3.2",
        "bufferutil": "^4.0.6",
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.1",
        "erlpack": "discord/erlpack",
        "fast-glob": "^3.2.12",
        "fastify": "^4.13.0",
        "htmlparser2": "^8.0.1",
        "node-gyp": "9.3.1",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.21.3",
        "sequelize-typescript": "^2.1.3",
        "sqlite3": "^5.0.11",
        "utf-8-validate": "^6.0.2",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.224",
        "@swc/helpers": "^0.4.3",
        "@tsconfig/node16": "^1.0.3",
        "@tsconfig/recommended": "^1.0.1",
        "@types/eslint": "^8",
        "@types/node": "^18.11.9",
        "@types/prettier": "^2",
        "@types/validator": "^13.7.5",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "eslint": "^8.21.0",
        "eslint-plugin-promise": "^6.0.0",
        "prettier": "2.8.4",
        "regenerator-runtime": "^0.13.9",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    }
}
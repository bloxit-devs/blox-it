{
    "name": "blox-it",
    "main": "src/bot.ts",
    "type": "module",
    "engines": {
        "node": ">=16.14.0 <19.0.0"
    },
    "scripts": {
        "start": "node --optimize_for_size --gc_interval=100 --max_old_space_size=920 dist/bot.js",
        "deploy": "node dist/deploy.js",
        "build": "swc ./src -d dist --delete-dir-on-start",
        "get-luau": "node get-luau.js",
        "dev": "cross-env-shell NODE_ENV=development \"pnpm build && pnpm run get-luau && pnpm run deploy && pnpm start\"",
        "type-check": "tsc",
        "lint": "eslint \"src/**/*.ts\"",
        "prettier": "prettier --write \"{get-luau,src/**/*}.{ts,js}\""
    },
    "dependencies": {
        "@hapi/boom": "^10.0.1",
        "@hapi/hapi": "^21.3.2",
        "axios": "^1.3.5",
        "bufferutil": "^4.0.6",
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.1",
        "erlpack": "discord/erlpack",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2",
        "htmlparser2": "^9.0.0",
        "joi": "^17.6.0",
        "node-gyp": "10.0.1",
        "pidusage": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.30.0",
        "sequelize-typescript": "^2.1.6",
        "sqlite3": "^5.1.6",
        "tempy": "^3.1.0",
        "utf-8-validate": "^6.0.3",
        "yauzl-promise": "^4.0.0",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.63",
        "@swc/core": "^1.3.99",
        "@swc/helpers": "^0.5.3",
        "@tsconfig/node18": "^18.2.2",
        "@tsconfig/recommended": "^1.0.1",
        "@types/emscripten": "^1.39.6",
        "@types/eslint": "^8",
        "@types/node": "^18.11.9",
        "@types/pidusage": "^2.0.5",
        "@types/validator": "^13.7.5",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.21.0",
        "eslint-plugin-promise": "^6.0.0",
        "prettier": "^3.1.0",
        "regenerator-runtime": "^0.14.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.3.2"
    },
    "volta": {
        "node": "18.18.2",
        "pnpm": "8.11.0"
    }
}